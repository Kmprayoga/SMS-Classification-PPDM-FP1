   "source": [
    "## Case Folding"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "metadata": {},
   "outputs": [],
   "source": [
    "import re\n",
    "\n",
    "# membuat fungsi untuk case folding\n",
    "def casefolding(text):\n",
    "    text = text.lower()                                 # merubah kalimat menjadi huruf kecil\n",
    "    text = re.sub(r'https?://\\S+|www\\.\\S+', '', text)   # menghapus url dari kalimat\n",
    "    text = re.sub(r'[-+]?[0-9]+', '', text)              # menghapus angka dari kalimat\n",
    "    text = re.sub(r'[^\\w\\s]', '', text)                 # menghapus tanda baca\n",
    "    text = text.strip()\n",
    "    return text"
   ]
  },
