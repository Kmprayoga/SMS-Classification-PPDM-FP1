    "\n",
    "loaded_vec = TfidfVectorizer(decode_error=\"replace\", vocabulary=set(pickle.load(open(\"new_selected_feature_tf-idf.sav\",\"rb\"))))\n",
    "hasil = best_model.predict(loaded_vec.fit_transform([data_input]))\n",
    "\n",
    "if(hasil==0):\n",
    "    s = \"sms normal\"\n",
    "elif(hasil==1):\n",
    "    s = \"sms penipuan\"\n",
    "else:\n",
    "    s = \"sms promo\"\n",
    "\n",
    "print(\"Hasil prediksi = \", s)\n",
    "\n"
