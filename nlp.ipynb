    "def text_normalize(text):\n",
    "    text = ' '.join([key_norm[key_norm['singkat'] == word]['hasil'].values[0]\n",
    "    if (key_norm['singkat'] == word).any()\n",
    "    else word for word in text.split()\n",
    "    ])\n",
    "\n",
    "    text = str.lower(text)\n",
    "    return text"
